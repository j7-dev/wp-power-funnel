parameters:
	level: 9
	paths:
		- plugin.php
		- inc/
	ignoreErrors:
		- '#Access to constant .* on an unknown class#'
		- '#Constant .* not found#'
		- '#Function .* not found#'
		- '#Cannot cast mixed to .*#'
		- '#Using nullsafe method call on non-nullable type .*#'
	reportUnmatchedIgnoredErrors: false
	excludePaths:
		- js/*
		- vendor/*
		- node_modules/*
	bootstrapFiles:
		- vendor/php-stubs/wordpress-stubs/wordpress-stubs.php
		- vendor/php-stubs/woocommerce-stubs/woocommerce-stubs.php
		- C:\Users\User\DEV\turborepo\powerrepo\apps\powerhouse/plugin.php
		- C:\Users\User\DEV\turborepo\powerrepo\apps\powerhouse\vendor\autoload.php
